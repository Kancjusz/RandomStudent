<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="RandomStudent.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:RandomStudent"
    Shell.FlyoutBehavior="Disabled"
    Title="RandomStudent">

    <Shell.Resources>
        <StyleSheet Source="/Resources/Styles/ShellStyle.css"/>
    </Shell.Resources>

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:Views.PickClass}"
        Route="MainPage" />
</Shell>
